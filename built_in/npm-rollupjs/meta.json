{
  "id": "npm-rollupjs",
  "args": [
    {
      "key": "project_name",
      "q_type": "Text",
      "label": "Project name"
    },
    {
      "key": "project_description",
      "q_type": "Text",
      "label": "Project description"
    },
    {
      "key": "author",
      "q_type": "Text",
      "label": "Author"
    },
    {
      "key": "out_dir",
      "q_type": "Text",
      "label": "Build output directory"
    },
    {
      "key": "access",
      "q_type": "Select",
      "label": "Npm package access",
      "options": ["public", "private"]
    },
    {
      "key": "license",
      "q_type": "Select",
      "label": "License",
      "options": ["MIT", "GPL"]
    }
  ],
  "dynamic_files": [
    "tsconfig.json",
    "tsconfig.build.json",
    "package.json",
    "rollup.config.js",
    "README.md",
    ".eslintignore",
    ".gitignore",
    "src/__tests__/sample.spec.ts",
    "src/index.ts"
  ]
}
